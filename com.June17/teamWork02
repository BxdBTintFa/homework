public class work2 {

    public static void main(String[] args) {
		// TODO Auto-generated method stub
		int b[] = new int[42];
		b[0] = 1;
		b[1] = 2;
		float sum = 0f;
		for (int i = 0; i < 40; i = i + 2) {
			b[i + 2] = b[i + 1];
			b[i + 3] = b[i + 1] + b[i];
			sum += (float) b[i + 1] / b[i];
		}
		System.out.println(sum);
	}
}
